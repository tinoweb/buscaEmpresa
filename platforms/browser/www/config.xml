<?xml version='1.0' encoding='utf-8'?>
<widget id="com.buscaempresa.phonega" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Pesquisa CNPJ</name>
    <description>
        Aplicativo Para Pesquisar Informações sobre CNPJ.
    </description>
    <author email="tino477@gmail.com" href="https://tinoweb.github.io/site/">
        Tinoweb
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
